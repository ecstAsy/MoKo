.nav-custom{
  
  width: 100%;
  top: 0;
  z-index: 9999;
}

.nav-custom-bar{
  width: 90%;
  padding: 0 5%;
  display: flex;
  flex-direction: row;
}

.nav-custom-bar-left{
  flex: 1;
}
.nav-custom-bar-middle{
  flex: 2;
  
}
.nav-custom-bar-right{
  flex: 1;
}

.nav-custom-bar-left button,
.nav-custom-bar-middle button{
  height: 100%;
  width: 100%;
  background: transparent;
  border: 1rpx solid transparent;
  border-radius: 0;
  outline: medium;
  padding: 0;
}
.nav-custom-bar-left button{
  text-align: left;
}
.nav-custom-bar-middle button{
  font-size: 34rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}
.nav-custom-bar-middle button text{
  width: 100%;
  vertical-align: middle;
}
.nav-custom-bar-left button::after,
.nav-custom-bar-middle button::after{
  border: 1rpx solid transparent;
  border-radius: 0;
  outline: medium;
}
.nav-custom-bar-left button image{
  width: 40%;
  vertical-align: middle;
}

